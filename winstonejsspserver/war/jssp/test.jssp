<table border="1px">
<tr><th>key</th><th>value</th></tr>
<% var keyList = new java.util.ArrayList(java.lang.System.getProperties().keySet());%>
<% for (var i = 0; i < keyList.size(); i++) { %>
  <% var k = keyList.get(i); %>
  <tr><td><%=k%></td><td><%=java.lang.System.getProperty(k)%></td></tr>
<% } %>
</table>
